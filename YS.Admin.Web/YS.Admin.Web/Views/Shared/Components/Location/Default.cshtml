@model IEnumerable<WebTreeList>
<div class="center clearfix">
    <div class="breadcrumb">
        当前位置：
        <span><a href="/">首页</a></span>
        @foreach (var item in Model)
        {
            <span>
                <a href="/@item.ModelUrl/C/@item.Cid">@item.Title</a>
            </span>
        }
    </div>
</div>
